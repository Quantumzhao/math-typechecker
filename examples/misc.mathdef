intersect := A' * B' -> C' where
	A' := Class typeof Set A
	B' := Class typeof Set B
	C' := Class typeof Set C
	C' ~ A' by subset
	C' ~ B' by subset

isIn := A ~ A' where
	A := Set A
	A' := Class A' typeof Set A

a := element of A

f := A -> B

f(a)
-----------------------------
f(a) = b where
	b isIn B

