Empty := Set Empty

isSubsetOf := A ~ B where
    A := AnySet
    B := AnySet

intersect := A' * B' -> C' where
    A' := AnySet
    B' := AnySet
    C' := AnySet
    C' isSubsetOf A
    C' isSubsetOf B

union := Set A * Set B -> Set C where
    A' isSubsetOf C'
    B' isSubsetOf C'
    A' isIn A
    B' isIn B
    C' isIn C
