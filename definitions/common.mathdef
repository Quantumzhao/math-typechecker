Empty := finite Set

intersect := (A', B') -> C' where
    A' := AnySet
    B' := AnySet
    C' := Intersection where
        Intersection isSubsetOf AnySet
    A' ~ C' by isSubsetOf
    B' ~ C' by isSubsetOf

union := (A, B) -> C where
    A' := AnySet
    B' := AnySet
    C' := AnySet
    C' ~ A' by isSubsetOf
    C' ~ B' by isSubsetOf

Natural := finite Set

Real := Set

Natural ~ Real by isSubsetOf

add := Real * Real -> Real
